<?xml version="1.0" encoding="UTF-8"?>
<Definitions xmlns="http://www.omg.org/spec/DMN/20130901"
             id="definitions"
             name="camunda"
             namespace="http://camunda.org/dmn">

  <ItemDefinition id="integerItem">
    <typeDefinition>integer</typeDefinition>
  </ItemDefinition>

  <ItemDefinition id="booleanItem">
    <typeDefinition>boolean</typeDefinition>
  </ItemDefinition>

  <Decision id="decision">
    <DecisionTable id="decisionTable" isComplete="true" isConsistent="true" hitPolicy="FIRST">
      <clause id="clause1" name="Type">
        <inputExpression id="inputExpression1">
          <text>type</text>
        </inputExpression>
        <inputEntry id="typed">
          <text>"typed"</text>
        </inputEntry>
        <inputEntry id="untyped">
          <text>"untyped"</text>
        </inputEntry>
      </clause>
      <clause id="clause2" name="Integer">
        <inputExpression id="inputExpression2">
          <itemDefinition href="#integerItem" />
          <text>integer</text>
        </inputExpression>
        <inputEntry id="typedInteger" expressionLanguage="juel">
          <text><![CDATA[${cellInput_typed.getValue() >= 42}]]></text>
        </inputEntry>
        <inputEntry id="untypedInteger" expressionLanguage="juel">
          <text><![CDATA[${cellInput >= 42}]]></text>
        </inputEntry>
      </clause>
      <clause id="clause3" name="Output">
        <outputDefinition href="#booleanItem" />
        <outputEntry id="match">
          <text>true</text>
        </outputEntry>
        <outputEntry id="else">
          <text>false</text>
        </outputEntry>
      </clause>
      <rule id="rule1">
        <condition>typed typedInteger</condition>
        <conclusion>match</conclusion>
      </rule>
      <rule id="rule2">
        <condition>untyped untypedInteger</condition>
        <conclusion>match</conclusion>
      </rule>
      <rule id="rule3">
        <condition>typed</condition>
        <conclusion>else</conclusion>
      </rule>
      <rule id="rule4">
        <condition>untyped</condition>
        <conclusion>else</conclusion>
      </rule>
    </DecisionTable>
  </Decision>
</Definitions>
