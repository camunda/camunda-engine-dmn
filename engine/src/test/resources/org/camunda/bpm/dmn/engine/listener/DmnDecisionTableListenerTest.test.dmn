<?xml version="1.0" encoding="UTF-8"?>
<Definitions xmlns="http://www.omg.org/spec/DMN/20130901"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="definitions"
             name="camunda"
             namespace="http://camunda.org/dmn">
  <Decision id="decision">
    <DecisionTable id="decisionTable" isComplete="true" isConsistent="true" hitPolicy="RULE ORDER">
      <clause id="input1" name="Input">
        <inputExpression id="inputExpression1">
          <text>${input1}</text>
        </inputExpression>
        <inputEntry id="inputEntry1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="inputEntry2">
          <text>false</text>
        </inputEntry>
      </clause>
      <clause id="input2" camunda:output="x">
        <inputExpression id="inputExpression2">
          <text>${input2}</text>
        </inputExpression>
        <inputEntry id="inputEntry3">
          <text>"foo"</text>
        </inputEntry>
        <inputEntry id="inputEntry4">
          <text>"bar"</text>
        </inputEntry>
      </clause>
      <clause id="input3">
        <inputEntry id="inputEntry5" expressionLanguage="juel">
          <text>${input3}</text>
        </inputEntry>
      </clause>
      <clause id="output1" name="Output 1" camunda:output="out1">
        <outputEntry id="outputEntry1">
          <text>${output1}</text>
        </outputEntry>
      </clause>
      <clause id="output2">
        <outputEntry id="outputEntry2">
          <text>"camunda"</text>
        </outputEntry>
      </clause>
      <clause id="output3" name="Collect Outputs" camunda:output="collectMe">
        <outputEntry id="outputEntry3">
          <text>10</text>
        </outputEntry>
        <outputEntry id="outputEntry4">
          <text>30</text>
        </outputEntry>
        <outputEntry id="outputEntry5">
          <text>50</text>
        </outputEntry>
      </clause>
      <rule id="rule1">
        <condition>inputEntry1 inputEntry3</condition>
        <conclusion>outputEntry1 outputEntry2</conclusion>
      </rule>
      <rule id="rule2">
        <condition>inputEntry2 inputEntry4</condition>
        <conclusion>outputEntry2</conclusion>
      </rule>
      <rule id="rule3">
        <condition>inputEntry5</condition>
      </rule>
      <rule id="rule4">
        <condition>inputEntry5</condition>
        <conclusion>outputEntry3</conclusion>
      </rule>
      <rule id="rule5">
        <condition>inputEntry5</condition>
        <conclusion>outputEntry4</conclusion>
      </rule>
      <rule id="rule6">
        <condition>inputEntry5</condition>
        <conclusion>outputEntry5</conclusion>
      </rule>
    </DecisionTable>
  </Decision>
</Definitions>
