<?xml version="1.0" encoding="UTF-8"?>
<Definitions xmlns="http://www.omg.org/spec/DMN/20130901"
             id="definitions"
             name="camunda"
             namespace="http://camunda.org/dmn">

  <ItemDefinition id="customItem">
    <typeDefinition>custom</typeDefinition>
  </ItemDefinition>

  <Decision id="decision">
    <DecisionTable id="decisionTable" isComplete="true" isConsistent="true" hitPolicy="FIRST">
      <clause id="clause">
        <outputDefinition href="customItem" />
        <outputEntry id="out">
          <text>${output}</text>
        </outputEntry>
      </clause>
      <rule id="rule">
        <conclusion>out</conclusion>
      </rule>
    </DecisionTable>
  </Decision>
</Definitions>
